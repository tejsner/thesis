@article{Reznik2006,
  title = {Electron\textendash{}Phonon Coupling Reflecting Dynamic Charge Inhomogeneity in Copper Oxide Superconductors},
  volume = {440},
  copyright = {2006 Nature Publishing Group},
  issn = {1476-4687},
  abstract = {The attempt to understand copper oxide superconductors is complicated by the presence of multiple strong interactions in these systems. Many believe that antiferromagnetism is important for superconductivity, but there has been renewed interest in the possible role of electron\textendash{}lattice coupling1,2,3,4. The conventional superconductor MgB2 has a very strong electron\textendash{}lattice coupling, involving a particular vibrational mode (phonon) that was predicted by standard theory and confirmed quantitatively by experiment5. Here we present inelastic scattering measurements that show a similarly strong anomaly in the Cu\textendash{}O bond-stretching phonon in the copper oxide superconductors La2-xSrxCuO4 (with x = 0.07, 0.15). Conventional theory does not predict such behaviour. The anomaly is strongest in La1.875Ba0.125CuO4 and La1.48Nd0.4Sr0.12CuO4, compounds that exhibit spatially modulated charge and magnetic order, often called stripe order6; it occurs at a wave vector corresponding to the charge order. These results suggest that this giant electron\textendash{}phonon anomaly, which is absent in undoped and over-doped non-superconductors, is associated with charge inhomogeneity. It follows that electron\textendash{}phonon coupling may be important to our understanding of superconductivity, although its contribution is likely to be indirect.},
  language = {en},
  number = {7088},
  journal = {Nature},
  doi = {10.1038/nature04704},
  author = {Reznik, D. and Pintschovius, L. and Ito, M. and Iikubo, S. and Sato, M. and Goka, H. and Fujita, M. and Yamada, K. and Gu, G. D. and Tranquada, J. M.},
  month = apr,
  year = {2006},
  keywords = {LBCO,LNSCO,LSCO,Phonon Anomaly,Neutron Scattering},
  pages = {1170-1173},
  file = {C:\\Users\\tejsnertb\\Dropbox\\wrk\\zotero\\Reznik2006.pdf;C:\\Users\\tejsnertb\\Zotero\\storage\\EEUWE2IM\\nature04704.html}
}

@article{Parlinski1997,
  title = {First-{{Principles Determination}} of the {{Soft Mode}} in {{Cubic ZrO2}}},
  volume = {78},
  abstract = {A direct approach to calculate the phonon dispersion using an ab initio force constant method is introduced. The phonon dispersion and structural instability of cubic ZrO2 are found using a supercell method in the local-density approximation. The force constants are determined from the Hellmann-Feynman forces induced by the displacement of an atom in the 2\texttimes{}2\texttimes{}2 fcc supercell. This size of the supercell gives ``exact'' phonon frequencies at {$\gamma$},X,L,W Brillouin zone points. The phonon dispersion curves show a pronounced soft mode at the X point, in agreement with the experimentally observed cubic to tetragonal phase transition.},
  number = {21},
  journal = {Phys. Rev. Lett.},
  doi = {10.1103/PhysRevLett.78.4063},
  author = {Parlinski, K. and Li, Z. Q. and Kawazoe, Y.},
  month = may,
  year = {1997},
  keywords = {DFT,PHONON,Lattice Dynamics},
