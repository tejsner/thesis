@article{Jacobsen2018,
  title = {Distinct {{Nature}} of {{Static}} and {{Dynamic Magnetic Stripes}} in {{Cuprate Superconductors}}},
  volume = {120},
  abstract = {We present detailed neutron scattering studies of the static and dynamic stripes in an optimally doped high-temperature superconductor, La2CuO4+y. We observe that the dynamic stripes do not disperse towards the static stripes in the limit of vanishing energy transfer. Therefore, the dynamic stripes observed in neutron scattering experiments are not the Goldstone modes associated with the broken symmetry of the simultaneously observed static stripes, and the signals originate from different domains in the sample. These observations support real-space electronic phase separation in the crystal, where the static stripes in one phase are pinned versions of the dynamic stripes in the other, having slightly different periods. Our results explain earlier observations of unusual dispersions in underdoped La2-xSrxCuO4 (x=0.07) and La2-xBaxCuO4 (x=0.095).},
  number = {3},
  journal = {Phys. Rev. Lett.},
  doi = {10.1103/PhysRevLett.120.037003},
  author = {Jacobsen, H. and Holm, S. L. and L{\u a}c{\u a}tu{\c s}u, M.-E. and R{\o}mer, A. T. and Bertelsen, M. and Boehm, M. and {Toft-Petersen}, R. and Grivel, J.-C. and Emery, S. B. and Udby, L. and Wells, B. O. and Lefmann, K.},
  month = jan,
  year = {2018},
  keywords = {Stripe Order,LCO+O,Neutron Scattering,Phase Separation},
  pages = {037003},
  file = {C\:\\Users\\ttejs\\Dropbox\\wrk\\zotero\\Jacobsen2018_supmat.pdf;C\:\\Users\\ttejs\\Dropbox\\wrk\\zotero\\Jacobsen2018.pdf;C\:\\Users\\ttejs\\Zotero\\storage\\TS56J55K\\PhysRevLett.120.html}
}

@article{Mohottala2006,
  title = {Phase Separation in Superoxygenated {{La2}}-{{xSrxCuO4}}+y},
  volume = {5},
  copyright = {2006 Nature Publishing Group},
  issn = {1476-4660},
  abstract = {The complex interplay between superconducting and magnetic phases remains poorly understood. Here, we report on the phase separation of doped holes into separate magnetic and superconducting regions in superoxygenated La2-xSrxCuO4+y, with various Sr contents. Irrespective of Sr-doping, excess oxygen raises the superconducting onset to 40 K with a coexisting magnetic spin-density wave that also orders near 40 K in each of our samples. The magnetic region is closely related to the anomalous, 1/8-hole-doped magnetic versions of La2CuO4, whereas the superconducting region is optimally doped. The two phases are probably the only truly stable ground states in this region of the phase diagram. This simple two-component system is a candidate for electronic phase separation in cuprate superconductors, and a key to understanding seemingly conflicting experimental observations.},
  number = {5},
  journal = {Nature Materials},
  doi = {10.1038/nmat1633},
  author = {Mohottala, Hashini E. and Wells, Barrett O. and Budnick, Joseph I. and Hines, William A. and Niedermayer, Christof and Udby, Linda and Bernhard, Christian and Moodenbaugh, Arnold R. and Chou, Fang-Cheng},
