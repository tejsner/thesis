  keywords = {LSCO,Phonon Anomaly,Charge Order,Neutron Scattering,ARPES,X-ray Scattering},
  pages = {020506},
  file = {C\:\\Users\\ttejs\\Dropbox\\wrk\\zotero\\Park2014.pdf;C\:\\Users\\ttejs\\Zotero\\storage\\BBKMCULD\\PhysRevB.89.html}
}

