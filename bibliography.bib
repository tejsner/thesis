  journal = {Phys. Rev. B},
  doi = {10.1103/PhysRevB.57.13915},
  author = {Blakeslee, P. and Birgeneau, R. J. and Chou, F. C. and Christianson, R. and Kastner, M. A. and Lee, Y. S. and Wells, B. O.},
